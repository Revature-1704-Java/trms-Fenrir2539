<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreateReimServlet</servlet-name>
  	<display-name>CreateReimServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.CreateReimServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>reimServlet</servlet-name>
  	<display-name>reimServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.reimServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReimManagerServlet</servlet-name>
  	<display-name>ReimManagerServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.ReimManagerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateReimServlet</servlet-name>
  	<display-name>UpdateReimServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.UpdateReimServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateReimServlet</servlet-name>
  	<url-pattern>/create.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>reimServlet</servlet-name>
  	<url-pattern>/reimServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReimManagerServlet</servlet-name>
  	<url-pattern>/ReimManagerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateReimServlet</servlet-name>
  	<url-pattern>/UpdateReimServlet</url-pattern>
  </servlet-mapping>
</web-app>
